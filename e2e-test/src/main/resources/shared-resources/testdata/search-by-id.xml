<?xml version="1.0" encoding="UTF-8"?>
<testData xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xsi:noNamespaceSchemaLocation="https://gitlab.alfresco.com/tas/tas-namespaces/raw/master/input-test-data.xsd">
	<users>
		<user id="1" name="search" password="password" />
	</users>

	<sites>
		<site id="site1" name="MyCustomSearchSite" createdBy="search"
			visibility="PUBLIC">
			<folders>
				<folder id="folder1" name="folder1" createdBy="search">
					<custom-model name="F:tas:folder" />
					<comments>
						<comment id="c1" value="my first folder comment"
							createdBy="admin" />
					</comments>
					<files>
						<file id="file1" name="f1.txt" createdBy="admin">
							<content>content of f1</content>
							<custom-model name="D:tas:document">
								<properties>
									<property name="tas:TextPropertyC" value="abcd" />
									<property name="tas:IntPropertyC" value="999" />
								</properties>
							</custom-model>
							<comments>
								<comment id="c2" value="my comment on f1 file"
									createdBy="admin" />
							</comments>
							<tags>
								<tag id="tag-1" value="tag1" createdBy="admin" />
								<tag id="tag-2" value="tag1" createdBy="admin" />
							</tags>
						</file>
						<file id="file2" name="f2.txt" createdBy="admin">
							<content>content of f1</content>
							<custom-model name="D:tas:document">
								<properties>
									<property name="tas:TextPropertyC" value="abcd" />
									<property name="tas:IntPropertyC" value="888" />
								</properties>
							</custom-model>
						</file>
						<file id="file3" name="f3.txt" createdBy="admin">
							<content>content of f1</content>
							<custom-model name="D:tas:document">
								<properties>
									<property name="tas:TextPropertyC" value="abcd" />
									<property name="tas:IntPropertyC" value="777" />
								</properties>
							</custom-model>
						</file>
					</files>
					<folders>
						<folder id="f11" name="folderChildOfFolder1" createdBy="admin" />
					</folders>
				</folder>
			</folders>
		</site>
	</sites>
	<queries>

		<query
			value="SELECT * FROM cmis:folder WHERE cmis:parentId = 'workspace://SpacesStore/NODE_REF[site1]' ORDER BY cmis:name DESC, cmis:creationDate ASC"
			expectedResults="1" />
		<query
			value="SELECT * FROM tas:document WHERE cmis:objectId &lt;&gt; 'workspace://SpacesStore/NODE_REF[site1]' AND tas:TextPropertyC = 'abcd' ORDER BY cmis:name ASC"
			expectedResults="1" />
		<query
			value="SELECT * FROM tas:document WHERE cmis:objectId &lt;&gt; 'workspace://SpacesStore/NODE_REF[site1]' AND tas:TextPropertyC = 'abcd' ORDER BY cmis:name ASC"
			expectedResults="1" />
		<query
			value="SELECT * FROM tas:document WHERE cmis:objectId &lt;&gt; 'workspace://SpacesStore/NODE_REF[site1]' AND tas:TextPropertyC = 'abcd' ORDER BY cmis:name DESC, cmis:creationDate ASC"
			expectedResults="1" />

		<query
			value="SELECT * FROM tas:document WHERE cmis:objectId='workspace://SpacesStore/NODE_REF[file1]'"
			expectedResults="1"></query>
		<query
			value="SELECT * FROM tas:folder WHERE cmis:baseTypeId = 'cmis:folder' and cmis:objectId='workspace://SpacesStore/NODE_REF[folder1]' ORDER BY cmis:name ASC"
			expectedResults="1" />
		<query
			value="SELECT * FROM tas:folder WHERE cmis:baseTypeId = 'cmis:folder' and cmis:objectId='workspace://SpacesStore/NODE_REF[folder1]' ORDER BY cmis:name DESC"
			expectedResults="1" />

		<query
			value="SELECT * FROM tas:folder WHERE cmis:baseTypeId = 'cmis:folder' and cmis:objectId='workspace://SpacesStore/NODE_REF[folder1]' ORDER BY cmis:name DESC, cmis:creationDate ASC"
			expectedResults="1" />
		<query value="SELECT * FROM tas:document WHERE tas:IntPropertyC = 999"
			expectedResults="1" />
		<query
			value="SELECT * FROM tas:document WHERE tas:IntPropertyC &lt;&gt; 999"
			expectedResults="2" />
		<query value="SELECT * FROM tas:document WHERE tas:IntPropertyC &lt; 800"
			expectedResults="1" />
	</queries>
</testData> 
